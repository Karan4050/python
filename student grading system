mark = int(input("enter your marks : "))

if mark>=90:
    print("you got an A+")

elif mark>=80:
    print("you got an A")

elif mark>=70:
    print("you got a B")

elif mark>=60:
    print("you got a C")

elif mark>=50:
    print("you got a D")

else:
    print("you failed the EXAM")

    if mark<0:
        print("this marks is invalid")



